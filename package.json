{
  "name": "wiki-link",
  "icon": "images/logo.png",
  "displayName": "Wiki Link",
  "description": "Search keyword on wiki and generate markdown link of the selected wiki page",
  "publisher": "willxing",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.47.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WillXing/vscode-wiki-link-search"
  },
  "author": {
    "name": "Will Xing",
    "url": "https://willxing.github.io/"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:wiki-link.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wiki-link.search",
        "title": "wiki link search"
      }
    ],
    "configuration": {
      "title": "WikiLink",
      "properties": {
        "wikilink.linknum": {
          "type": "number",
          "default": 5,
          "description": "How many links want to show in every search, from 1 to 20",
          "maximum": 20,
          "minimum": 1
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
